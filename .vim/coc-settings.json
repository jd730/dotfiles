{
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "ℹ",
    "diagnostic.hintSign": "➤",
	"diagnostic.messageTarget": false,
    "suggest.noselect": false,
    "suggest.echodocSupport": true,
	"suggest.maxCompleteItemCount": 0,
    "codeLens.enable": true,
    "signature.enable": true,
	//"signature.target": "echo",
	"coc.preferences.hoverTarget": "echo",
	"suggest.floatEnable": false,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "typescript.preferences.quoteStyle": "single",
    "typescript.preferences.importModuleSpecifier": "auto",
    "eslint.autoFixOnSave": false,
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "html.suggest.html5": true,
    "python.venvFolders": ["envs",".pyenv",".direnv", "~/.local/share/virtualenvs", "~/.cache/pypoetry/virtualenvs"],
    "python.workspaceSymbols.exclusionPatterns": [],
    "python.jediEnabled": true,
	"pyright.enable": false,
	"pyright.completion.snippetSupport": false,
	"python.analysis.autoImportCompletions": false,
	"python.analysis.autoSearchPaths": false,
	"python.linting.enabled": false,
    "python.linting.pylintEnabled": false,
    "python.linting.pylintArgs": ["--load-plugins", "pylint_django"],
    "python.formatting.blackPath":"~/.local/bin/black",
    "python.linting.mypyEnabled": false,
    "python.linting.mypyPath": "~/.local/bin/mypy",
    "python.formatting.provider": "black",
    "python.sortImports.path": "/home/calev/.local/bin/isort",
    "git.addedSign.text": "▎",
    "git.changedSign.text": "▎",
    "git.removedSign.text": "▎",
    "git.changeRemovedSign.text": "▶",
    "git.topRemovedSign.text": "◥",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 40,
    "explorer.keyMappings.global": {
       "<right>": "expand",
       "<left>": "shrink"
    },
    "explorer.quitOnOpen": true
}

